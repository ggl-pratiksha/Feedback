# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

GET     /renderAddFeedback          controllers.AddController.renderAddFeedback

POST     /add                       controllers.AddController.addFeedback

GET     /searchRenderer             controllers.SearchController.searchRenderer

POST    /search                     controllers.SearchController.search

POST    /delete                     controllers.DeleteController.delete

GET     /deleteRenderer             controllers.DeleteController.deleteRenderer

GET     /updateRenderer             controllers.UpdateController.updateRenderer

POST    /update                     controllers.UpdateController.update

GET     /viewDB                     controllers.SearchController.viewDB
